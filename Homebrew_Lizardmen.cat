    </profile>
    <profile name="Soteks Poison" typeId="cb92-d882-5ba8-11ab" typeName="Spell" hidden="false" id="43b0-1619-547a-260f">
      <characteristics>
        <characteristic name="Spell Level" typeId="576b-eaa1-0bd1-3f7a">Innate Bound Spell</characteristic>
